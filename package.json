{
  "name": "websupport",
  "version": "0.0.2",
  "license": "MIT",
  "homepage": "https://github.com/JanSmolko/websupport",
  "repository": {
    "type": "git",
    "url": "https://github.com/JanSmolko/websupport.git"
  },
  "author": "Jan Smolko",
  "type": "module",
  "engines": {
    "node": ">=14.16"
  },
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "tsc-watch -p ./tsconfig.json --onSuccess \"node ./dist/index.js\"",
    "clean": "rimraf ./dist",
    "build": "yarn clean && yarn build:esm && yarn build:cjs && ./fixup",
    "build:esm": "tsc --module esnext --outDir dist/esm",
    "build:cjs": "tsc --module commonjs --outDir dist/cjs",
    "lint": "eslint --ignore-path .eslintignore --ext .js,.ts ."
  },
  "devDependencies": {
    "@types/crypto-js": "^4.1.1",
    "@types/node": "^18.11.18",
    "@typescript-eslint/eslint-plugin": "^5.47.0",
    "@typescript-eslint/parser": "^5.47.0",
    "dotenv": "^16.0.3",
    "eslint": "^8.30.0",
    "eslint-config-prettier": "^8.5.0",
    "prettier": "^2.8.1",
    "rimraf": "^3.0.2",
    "ts-node": "^10.9.1",
    "tsc-watch": "^6.0.0",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "axios": "^1.2.2",
    "crypto-js": "^4.1.1"
  }
}
